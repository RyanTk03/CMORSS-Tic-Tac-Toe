  "dependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongoose": "^8.5.3",
    "mongoose-unique-validator": "^5.0.1",
    "rimraf": "^6.0.1"
  }
